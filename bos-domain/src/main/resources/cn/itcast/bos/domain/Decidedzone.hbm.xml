<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.itcast.bos.domain">
    <class name="Decidedzone" table="bc_decidedzone">
        <id name="id">
            <generator class="native" />
        </id>
        <property name="name" ></property>
        <many-to-one lazy="false" name="staff" column="staff_id" class="Staff"></many-to-one>
        <set name="subareas" inverse="true">
        	<key column="decidedzone_id"></key>
        	<one-to-many class="Subarea"/>
        </set>
    </class>
</hibernate-mapping>
